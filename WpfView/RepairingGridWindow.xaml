<Window x:Class="WpfView.RepairingGridWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfView"
        mc:Ignorable="d" Height="393" Width="433"
        Title="">
    <Grid>
        <DataGrid x:Name="repairingsDataGrid" Margin="0,0,0,69" AutoGeneratingColumn="HideIdColumn" CanUserAddRows="True" ItemsSource="{Binding Repairings}"/>
        <Button x:Name="saveButton" Content="Подтвердить" HorizontalAlignment="Center" VerticalAlignment="Bottom" Height="29" Width="87" Click="saveButton_Click" Margin="0,325,0,0"/>
    </Grid>
</Window>
